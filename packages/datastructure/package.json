{
  "name": "@ultify/datastructure",
  "version": "0.2.2",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "license": "MIT",
  "sideEffects": false,
  "repository": "git@github.com:trinhthinh388/ultify.git",
  "description": "A Typescript/Javascript implementation of datastructures.",
  "author": "Thinh Trinh Duc <thinh.duc.trinh@gmail.com>",
  "keywords": [
    "typescript",
    "javascript",
    "algorithm",
    "datastructure"
  ],
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "vite --config ../../vite.config.ts build",
    "dev": "vite --watch --config ../../vite.config.ts build",
    "test": "vitest --config ../../vite.config.ts --coverage --run",
    "test:watch": "vitest --config ../../vite.config.ts --coverage --watch",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@ultify/eslint-config": "*",
    "@ultify/typescript-config": "*",
    "typescript": "^5.4.5",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  }
}
